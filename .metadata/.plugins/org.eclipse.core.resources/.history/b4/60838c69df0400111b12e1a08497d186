package nodes;

import util.models.ListenableVector;
import bus.uigen.shapes.SimpleShape;

public interface VisualizerElement<ElementType> extends SimpleShape, LinkedTree<ElementType>
{	
	public void focusPosition(ElementType node);
	public void focusPosition();
	
	public SimpleShape getShape();
	
	public ListenableVector<ElementType> getVector();

}